{
  "name": "maeum",
  "version": "1.4.0",
  "description": "Delicious fastify.js boilerplate",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "artifact": "just --config ./.configs/just.config.ts artifact",
    "thin-artifact": "just --config ./.configs/just.config.ts thin-artifact",
    "build": "just --config ./.configs/just.config.ts build",
    "pack:dev": "just --config ./.configs/just.config.ts pack:dev",
    "pack": "just --config ./.configs/just.config.ts pack",
    "clean": "just --config ./.configs/just.config.ts clean",
    "dev": "nodemon --config ./.configs/nodemon.json",
    "debug": "cross-env DEBUG=maeum:* ENV_APPLICATION_LOG_LEVEL=debug TS_NODE_PROJECT=tsconfig.json node -r ts-node/register -r src/configs/loader --inspect-brk --nolazy src/listen.ts",
    "just": "just --config ./.configs/just.config.ts",
    "lint": "just --config ./.configs/just.config.ts lint",
    "lint-staged": "lint-staged",
    "prettier": "just --config ./.configs/just.config.ts prettier",
    "lnb": "concurrently \"npm run build\" \"npm run lint\"",
    "route": "cross-env NODE_ENV=production fast-maker route --config ./.configs/.fastmakerrc",
    "start:withoutpm2": "TZ=Asia/Seoul node --enable-source-maps -r ./dist/loader dist/listen.js",
    "start": "TZ=Asia/Seoul ./node_modules/pm2/bin/pm2-runtime -s start .configs/pm2.config.js > /dev/null",
    "test": "cross-env DEBUG=maeum:* RUN_ON_TESTCASE=true node -r ts-node/register ./node_modules/.bin/jest --detectOpenHandles",
    "tsc": "just --config ./.configs/just.config.ts tsc",
    "typeorm": "cross-env DISABLE_RELATION_MODEL=on ts-node -r tsconfig-paths/register -r scripts/typeorm ./node_modules/typeorm/cli.js",
    "nozzle": "nozzle -c ./.configs/.nozzlerc",
    "dd": "deps-diff md -d prod -d dev --title-list-type '*' --deps-list-type '*' --content-list-type '*'",
    "uid": "node scripts/uuid.js",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/maeumjs/maeum.git"
  },
  "author": "ByungJoon Lee",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-typescript": "^11.1.0",
    "@swc/core": "^1.3.46",
    "@swc/helpers": "^0.5.0",
    "@swc/jest": "^0.2.24",
    "@tsconfig/node14": "^1.0.3",
    "@types/archiver": "^5.3.2",
    "@types/debug": "^4.1.7",
    "@types/figlet": "^1.5.5",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/node-polyglot": "^2.4.2",
    "@types/shelljs": "^0.8.11",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "archiver": "^5.3.1",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "deps-diff": "^1.0.2",
    "eslint": "^8.38.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^40.1.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "execa": "^5.1.1",
    "fast-maker": "^3.2.0",
    "figlet": "^1.5.2",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "just-task": "^1.7.2",
    "lint-staged": "^13.2.1",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "prettier-eslint": "^15.0.1",
    "prettier-plugin-organize-imports": "^3.2.2",
    "read-pkg": "^5.2.0",
    "rimraf": "^4.4.1",
    "rollup": "^3.20.2",
    "rollup-plugin-swc3": "^0.8.1",
    "schema-nozzle": "^0.22.0",
    "shelljs": "^0.8.5",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@fastify/cors": "^8.2.1",
    "@fastify/multipart": "^7.6.0",
    "@fastify/swagger": "^8.3.1",
    "@fastify/swagger-ui": "^1.7.0",
    "@fastify/url-data": "^5.3.1",
    "@maeum/error-handler": "^0.8.0",
    "@maeum/plugins": "^0.8.0",
    "accept-language": "^3.0.18",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "chalk": "^4.1.2",
    "change-case": "^4.1.2",
    "dayjs": "^1.11.7",
    "debug": "^4.3.4",
    "dotenv": "^16.0.3",
    "error-stack-parser": "^2.1.4",
    "fast-json-stringify": "^5.7.0",
    "fast-safe-stringify": "^2.1.1",
    "fastify": "^4.15.0",
    "fastify-plugin": "^4.5.0",
    "http-status-codes": "^2.2.0",
    "jin-curlize": "^0.5.0",
    "jin-frame": "^3.9.0",
    "json-schema": "^0.4.0",
    "jsonc-parser": "^3.2.0",
    "my-easy-fp": "^0.20.3",
    "my-node-fp": "^0.8.1",
    "node-polyglot": "^2.5.0",
    "path-to-regexp": "^6.2.1",
    "pm2": "^5.3.0",
    "snappy": "^7.2.2",
    "type-fest": "^3.8.0",
    "url-naong": "^1.0.0",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "cross-env NODE_ENV=production eslint --cache"
    ],
    "*.{js,jsx,ts,tsx},*.json,.{eslintrc.cjs,prettierrc}": [
      "cross-env NODE_ENV=production prettier --ignore-path .eslintignore --parser json --write"
    ]
  }
}
